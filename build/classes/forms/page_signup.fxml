<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="682.0" stylesheets="@../css/logIn.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Page_signup">
   <children>
      <ImageView fitHeight="569.0" fitWidth="755.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/bg-01.jpg" />
         </image>
      </ImageView>
      <AnchorPane id="anchPane_login" layoutX="211.0" layoutY="48.0" prefHeight="458.0" prefWidth="334.0">
         <children>
            <Text fill="WHITE" layoutX="108.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sign Up" textAlignment="CENTER">
               <font>
                  <Font name="HoloLens MDL2 Assets" size="33.0" />
               </font>
            </Text>
            <TextField id="tf_username" fx:id="tf_username" layoutX="45.0" layoutY="151.0" prefHeight="35.0" prefWidth="244.0" promptText="Enter username. . .">
               <font>
                  <Font name="Candara" size="14.0" />
               </font>
            </TextField>
            <TextField id="tf_email" fx:id="tf_email" layoutX="45.0" layoutY="201.0" prefHeight="35.0" prefWidth="244.0" promptText="Enter email id. . .">
               <font>
                  <Font name="Candara" size="14.0" />
               </font>
            </TextField>
            <PasswordField id="tf_password" fx:id="tf_password" layoutX="45.0" layoutY="251.0" prefHeight="35.0" prefWidth="244.0" promptText="Enter password. . .">
               <font>
                  <Font name="Candara" size="14.0" />
               </font>
            </PasswordField>
            <PasswordField id="tf_confirm" fx:id="tf_confirm" layoutX="45.0" layoutY="301.0" prefHeight="35.0" prefWidth="244.0" promptText="Confirm password. . .">
               <font>
                  <Font name="Candara" size="14.0" />
               </font>
            </PasswordField>
            <Button id="btn_signup" fx:id="btn_signup" layoutX="97.0" layoutY="361.0" mnemonicParsing="false" onAction="#btnSignup" prefHeight="35.0" prefWidth="141.0" text="Sign Up">
               <font>
                  <Font name="Cambria" size="16.0" />
               </font></Button>
            <Hyperlink id="link_login" layoutX="107.0" layoutY="412.0" onAction="#gotoLogin" text="I'm already registerd!" textFill="#f8f0f0" />
         </children></AnchorPane>
   </children>
</AnchorPane>
